{
	"manifest_version": 2,
	"name": "DaltonTab",
	"version": "0.3.1",

	"icons": {"128": "128.png"},

	"background": {
		"scripts": ["js/bg.js"]
	},

	"permissions": [
		"storage",
		"*://*.dalton.org/",
		"*://*.planhub.me/",
		"*://*.myhomework.space/",
		"https://www.bing.com/"
	],

	"chrome_url_overrides" : {
		"newtab": "newTab.html"
	},
	"browser_action": {
		"default_icon": {
			"64": "64.png",
			"128": "128.png"
		},
		"default_title": "DaltonTab",
		"default_popup": "newTab.html"
	}
}
