{
	"manifest_version": 2,
	"name": "DaltonTab",
	"version": "0.7.0",

	"icons": {"128": "img/128.png"},

	"background": {
		"scripts": ["bg.js"]
	},

	"permissions": [
		"storage",
		"*://*.dalton.org/",
		"*://*.myhomework.space/",
		"https://www.bing.com/"
	],

	"chrome_url_overrides" : {
		"newtab": "newTab.html"
	}
}
