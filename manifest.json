{
	"manifest_version": 2,
	"name": "DaltonTab",
	"version": "0.2.2",

	"icons": {"128": "128.png"},

	"background": {
		"scripts": ["js/bg.js"]
	},

	"permissions": [
		"storage",
		"*://*.dalton.org/",
		"*://*.planhub.me/",
		"https://www.bing.com/"
	],

	"chrome_url_overrides" : {
		"newtab": "newTab.html"
	}
}
